{
  "name": "@cliply/shared",
  "private": true,
  "version": "0.0.1",

  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",

  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.js",
      "types": "./dist/env.d.ts"
    },
    "./auth/*": {
      "import": "./dist/src/auth/*.js",
      "require": "./dist/src/auth/*.js",
      "types": "./dist/src/auth/*.d.ts"
    },
    "./observability/*": {
      "import": "./dist/src/observability/*.js",
      "require": "./dist/src/observability/*.js",
      "types": "./dist/src/observability/*.d.ts"
    },
    "./crypto/*": {
      "import": "./dist/src/crypto/*.js",
      "require": "./dist/src/crypto/*.js",
      "types": "./dist/src/crypto/*.d.ts"
    },
    "./logging/*": {
      "import": "./dist/logging/*.js",
      "require": "./dist/logging/*.js",
      "types": "./dist/logging/*.d.ts"
    },
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.js",
      "types": "./dist/lib/*.d.ts"
    },
    "./billing/*": {
      "import": "./dist/billing/*.js",
      "require": "./dist/billing/*.js",
      "types": "./dist/billing/*.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    },
    "./pipeline/*": {
      "import": "./dist/pipeline/*.js",
      "require": "./dist/pipeline/*.js",
      "types": "./dist/pipeline/*.d.ts"
    },
    "./storage/*": {
      "import": "./dist/storage/*.js",
      "require": "./dist/storage/*.js",
      "types": "./dist/storage/*.d.ts"
    }
  },

  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -b",
    "lint": "eslint . --ext .ts"
  },

  "dependencies": {
    "@sentry/node": "^8.42.0",
    "dotenv": "^17.2.3"
  },

  "devDependencies": {
    "@types/node": "^24.5.2",
    "stripe": "^19.2.0"
  }
}


